{
  "model": {
    "type": "minecraft:range_dispatch",
    "property": "minecraft:custom_model_data",
    "entries": [
      {
        "threshold": 1.0,
        "model": {
          "type": "minecraft:select",
          "cases": [
            {
              "model": {
                "type": "minecraft:model",
                "model": "dodgebolters:item/general/unqueued"
              },
              "when": ["gui", "ground", "fixed"]
            }
          ],
          "fallback": {
            "type": "minecraft:model",
            "model": "dodgebolters:item/inventory/unqueued_item"
          },
          "property": "minecraft:display_context"
        }
      },
      {
        "threshold": 2.0,
        "model": {
          "type": "minecraft:select",
          "cases": [
            {
              "model": {
                "type": "minecraft:model",
                "model": "dodgebolters:item/general/queued"
              },
              "when": ["gui", "ground", "fixed"]
            }
          ],
          "fallback": {
            "type": "minecraft:model",
            "model": "dodgebolters:item/inventory/queued_item"
          },
          "property": "minecraft:display_context"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "item/disc_fragment_5"
    }
  },
  "oversized_in_gui": true
