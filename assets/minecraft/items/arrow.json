{
  "model":{
    "type": "minecraft:range_dispatch",
    "property": "minecraft:custom_model_data",
    "entries": [
      {
        "threshold": 1.0,
        "model": {
          "type": "minecraft:select",
          "cases": [
            {
              "model": {
                "type": "minecraft:model",
                "model": "dodgebolters:item/cosmetics/bow_skins/singularity/arrow"
              },
              "when": [
                "none",
                "fixed"
              ]
            }
          ],
          "fallback": {
            "type": "minecraft:model",
            "model": "dodgebolters:item/cosmetics/bow_skins/singularity/arrow_gui"
          },
          "property": "minecraft:display_context"
        }
      },
      {
        "threshold": 2.0,
        "model": {
          "type": "minecraft:select",
          "cases": [
            {
              "model": {
                "type": "minecraft:model",
                "model": "dodgebolters:item/cosmetics/bow_skins/brow/arrow"
              },
              "when": [
                "none",
                "fixed"
              ]
            }
          ],
          "fallback": {
            "type": "minecraft:model",
            "model": "dodgebolters:item/cosmetics/bow_skins/brow/arrow_gui"
          },
          "property": "minecraft:display_context"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "item/arrow"
    }
  },
  "oversized_in_gui": true
}